{
  "Settings": {
    "SettingServerUrl": "https://localhost:7023"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "configSections": {
    "section": {
      "-name": "log4net",
      "-type": "log4net.Config.Log4NetConfigurationSectionHandler, log4net",
      "-self-closing": "true"
    }
  },
  "AllowedHosts": "*"
}
