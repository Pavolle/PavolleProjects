<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="appsettings.ini" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="appsettings.ini">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DevExpress.Xpo" Version="23.1.5" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="7.0.11" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Business\Project\Common\Enums\" />
    <Folder Include="Business\Project\Common\Utils\" />
    <Folder Include="Business\Project\DbModels\" />
    <Folder Include="Business\Project\MobileApp\" />
    <Folder Include="Business\Project\WebApp\" />
    <Folder Include="Business\Project\ViewModels\" />
    <Folder Include="Business\Project\WebSecurity\" />
  </ItemGroup>

</Project>